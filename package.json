{
  "name": "bark-ai-agent-web3",
  "version": "0.1.0",
  "description": "BARK AI Agent is an advanced AI-driven platform that streamlines charitable incentives and enhances trading experiences on the Solana blockchain.",
  "author": "BARK Protocol",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "postinstall": "pnpm rebuild",
    "dev": "NODE_NO_WARNINGS=1 next dev",
    "build": "next build",
    "start": "next start",
    "vercel-build": "NODE_OPTIONS=--max_old_space_size=2048 next build",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md,json,js,jsx}\"",
    "db:reset": "pnpm npx prisma generate && pnpm npx prisma migrate reset && pnpm npx prisma db push",
    "generate": "prisma generate",
    "migrate": "prisma migrate deploy",
    "dev:up-build": "docker compose up --build -d",
    "dev:up": "docker compose up -d"
  },
